{
  "settings": {
    "index.refresh_interval": "5s",
    "number_of_shards" : "1",
    "number_of_replicas": "0",
    "dynamic": false
  },
  "analysis": {
    "analyzer": {
      "std_english": { 
        "type":      "standard",
        "stopwords": "_english_"
      }
    }
  },
  "mappings": {
    "properties" : {
      "@timestamp" : {
        "type" : "date"
      },
      "@version" : {
        "type" : "integer"
      },
      "category" : {
        "type" : "keyword"
      },
      "click_time" : {
        "type" : "date"
      },
      "query" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      },
      "query_time" : {
        "type" : "date"
      },
      "sku" : {
        "type" : "integer"
      },
      "user" : {
        "type" : "keyword"
      }
    }
  }
}